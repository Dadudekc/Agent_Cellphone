# GitHub auth
GITHUB_USERNAME=
GITHUB_TOKEN=
DISCORD_WEBHOOK_URL=
DEVLOG_USERNAME=
# Force non-interactive git (prevents popups)
GIT_TERMINAL_PROMPT=0
GCM_INTERACTIVE=Never

# Optional: verbose HTTP logging from git
GIT_CURL_VERBOSE=1

# Clone destination used by scripts
REPO_DEST=D:\Agent_Cellphone\repos

